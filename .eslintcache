        projectPath = destPath;\n            return FileManager.createFolder(destPath);\n        })\n        .then((destPath) => {\n            if (file == null) {\n                return Promise.reject();\n            }\n            return FileManager.saveFile(file.path, destPath + \"/\" + file.name);\n        })\n        .then((path) => {\n            props.handleSrc({\n                media: {\n                    url: URL.createObjectURL(file),\n                    name: file.name\n                },\n                isFromYoutube: false,\n                projectPath: projectPath\n            });\n            setRedirect(true);\n        })\n        .catch(()=>{\n            setFileError(\"Error saving the file. Try with another project folder name.\");\n        });\n    };\n\n    const handleVideoFile = (files) => {\n        const file = files[0];\n        const extension = FileManager.getFileExtension(file.name);\n        if (validVideoExtensions.includes(extension) || validImageExtensions.includes(extension)) {\n            createProject(file);\n        } else {\n            setFileError(\"File error: make sure that the video format is MP4.\")\n        }\n    };\n\n    const handleYoutubeURL = () => {\n        if (FileManager.isYoutubeURLValid(state.youtubeURL)) {\n            FileManager.showSaveDialog(\"Create your project folder...\", \"Project name:\", \"Create\", \"documents\")\n            .then((destPath) => {\n                return FileManager.createFolder(destPath);\n            })\n            .then((destPath) => {\n                props.handleSrc({\n                    media: state.youtubeURL,\n                    isFromYoutube: true,\n                    projectPath: destPath\n                });\n                setRedirect(true);\n            }).catch(() => {\n                setYoutubeError(\"Error creating the project. Try with another folder name.\");\n            });\n        } else {\n            setYoutubeError(\"Please enter a youtube ID\");\n        }\n    };\n\n    const renderRedirect = () => {\n        if (state.needsToRedirect) {\n            return <Redirect to=\"/editor\"/>;\n        }\n    };\n\n    const handleTFChange = (value) => {\n        setState((prevState) => {\n            return {\n                ...prevState,\n                youtubeURL: value\n            };\n        });\n    };\n\n    return (\n        <div className=\"welcome-page\">\n            {renderRedirect()}\n            <p className=\"welcome-title\">Upload your video file</p>\n            <DragAndDrop text=\"Drag and drop your mp4 file\" handleDrop={handleVideoFile}/>\n            <p className=\"error-text\">{state.fileError}</p>\n            <p className=\"welcome-title\">choose a video from youtube</p>\n            <RoundTextField className=\"youtube-tf\" placeholder=\"Youtube video ID\" handleChange={handleTFChange}>\n                <MainButton title=\"DONE\" disabled={false} handleClick={handleYoutubeURL}/>\n            </RoundTextField>\n            <p className=\"error-text\">{state.youtubeError}</p>\n        </div>\n    );\n\n};\n\n\nexport default WelcomePage;","/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/navbar/SideNavBar.js",[],"/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/editor/EditorPage.js",["112"],"/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/welcome/drag-and-drop/DragAndDrop.js",[],"/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/components/round-tf/RoundTextField.js",[],"/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/components/main-button/MainButton.js",[],"/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/editor/hypervideo/Hypervideo.js",[],"/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/editor/tag-label/TagLabel.js",[],"/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/components/link-button/LinkButton.js",[],"/Users/aleixdiaz/Documents/Hypervideos/Hiper/hiper/src/components/setting-tf/SettingTextfield.js",[],"/Users/aleixdiaz/Documen